{
  "Kestrel": {
    "Endpoints": {
      "ep1": {
        "Url": "http://127.0.0.1:6601"
      },
      "ep2": {
        "Url": "http://[::]:6601"
      }
    }
  },
  // 主机配置
  "Hosting": {
    // 插件查找目录列表
    "Paths": [
      "~/libs"
    ],
    // 插件列表
    "Libraries": [],
    // 日志目录
    "LogPath": "~/logs",
    // 是否输出详细日志
    "IsVerboseLog": true,
    // 是否允许跨域
    "IsCorsAll": true,
    // 是否允许显示Swagger
    "IsSwagger": true,
    // Swagger分类配置
    "Swaggers": [
      {
        "Name": "all",
        "Description": "所有接口",
        "Keyword": "*"
      },
      {
        "Name": "user",
        "Description": "用户模块(User)接口",
        "Keyword": "Suyaa.Configure.App.Users"
      },
      {
        "Name": "setup",
        "Description": "安装模块(Setup)接口",
        "Keyword": "Suyaa.Configure.App.Setups"
      }
    ]
  },
  // 数据库连接
  "ConnectionStrings": {
    "Hosting": "Data Source=./config.db;Version=3;"
  }
}