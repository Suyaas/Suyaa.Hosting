{
  "Kestrel": {
    "Endpoints": {
      "ep1": {
        "Url": "http://127.0.0.1:6600"
      },
      "ep2": {
        "Url": "http://[::]:6600"
      }
    }
  },
  // 主机配置
  "Hosting": {
    "Paths": [
      "~/libs"
    ],
    "Libraries": [],
    "LogPath": "~/logs",
    "IsVerboseLog": true,
    "IsCorsAll": true,
    "IsSwagger": true,
    "Swaggers": [
      {
        "Name": "all",
        "Description": "All APIs",
        "Keyword": "*"
      },
      {
        "Name": "user",
        "Description": "User",
        "Keyword": "Suyaa.Apis.User"
      }
    ]
  },
  // 数据库连接
  "ConnectionStrings": {
    "Hosting": "Data Source=./config.db;Version=3;"
  }
}